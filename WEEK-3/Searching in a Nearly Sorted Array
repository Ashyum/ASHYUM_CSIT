class Solution
{
    //Function to return the sorted array.
    ArrayList <Integer> nearlySorted(int arr[], int num, int k)
    {
        Arrays.sort(arr);
        ArrayList<Integer> list = new ArrayList();
        for(int i=0;i<num;i++)
        {
            list.add(arr[i]);
        }
        
        return list;
    }
}
